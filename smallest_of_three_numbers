first_number = int(input())
second_number = int(input())
third_number = int(input())

def smallest_number(number1, number2, number3):
    if second_number > first_number < third_number:
        return first_number
    elif first_number > second_number < third_number:
        return second_number
    elif first_number > third_number < second_number:
        return third_number
print(smallest_number(first_number, second_number, third_number))
